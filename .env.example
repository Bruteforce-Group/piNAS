# piNAS Deployment Environment Variables
# Copy this file to .env and fill in your actual values

# Cloudflare Worker Configuration
WORKER_URL="https://pinas-deployer.YOUR_ACCOUNT.workers.dev"
WORKER_ADMIN_TOKEN="generate-with-wrangler-secret-put"

# Cloudflare R2 Bucket Name
PINAS_R2_BUCKET="pinas-artifacts"

# Optional: Environment (production or development)
PINAS_ENV="production"

# SSH Configuration (optional - for manage-clients.sh)
PINAS_SSH_KEY="$HOME/.ssh/pinas-deploy-key"
PINAS_SSH_USER="pi"
